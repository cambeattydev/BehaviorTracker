@page "/Admin/Users"
@inject HttpClient _httpClient

<h3>Users</h3>
<div class="row">
    <div class="col">
        <div class="list-group">
        </div>
    </div>
</div>

@functions
{

    protected IList<BehaviorTrackerUser> Users { get; set; }

    protected override async Task OnInitAsync()
    {
        Users = await _httpClient.GetJsonAsync<IList<BehaviorTrackerUser>>("/api/BehaviorTrackerUser/Users");
    }

}
